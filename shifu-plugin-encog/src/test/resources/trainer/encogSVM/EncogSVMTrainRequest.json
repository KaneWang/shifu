{
    "version": "0.3.0-SNAPSHOT",
    "requests": [
        {
            "requestType": "ExecTrain",
            "executionMode": "LOCAL_SINGLE",
            "globalParams": {
                "bindings": {
                    "Trainer": "ml.shifu.plugin.encog.trainer.EncogSVMTrainer"
                },
                "modelName": "demoModel",
                "pathPMML": "src/test/resources/trainer/model.xml",
                "pathNormalizedData": "src/test/resources/trainer/normalized.txt",
                "numEpochs": 100,
                "trainerID": "test",
                "pathOutput": "src/test/resources/trainer/encogSVM/modelOutput.svm",
                "splitRatio": 0.8,
                "algorithm": "Q",
                "learningRate": 0.5,
                "kernel": "rbf",
                "constant": 1.1,
                "gamma": 0.95
            }
        
        }
    ]
}