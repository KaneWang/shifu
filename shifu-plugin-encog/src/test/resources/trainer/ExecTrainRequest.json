{
    "version": "0.3.0-SNAPSHOT",
    "requests": [
        {
            "requestType": "ExecTrain",
            "executionMode": "LOCAL_SINGLE",
            "globalParams": {
                "bindings": {
                    "Trainer": "ml.shifu.plugin.encog.trainer.EncogNNTrainer"
                },
                "modelName": "demoModel",
                "pathPMML": "src/test/resources/trainer/model.xml",
                "pathNormalizedData": "src/test/resources/trainer/normalized.txt",
                "numEpochs": 100,
                "trainerID": "test",
                "pathOutput": "tmp/generated/models",
                "splitRatio": 0.8,
                "algorithm": "Q",
                "learningRate": 1.0,
                "hiddenLayers": [
                    {
                        "numHiddenNodes": 20,
                        "activationFunction": "sigmoid"
                    }
                ]
            }
        
        }
    ]
}